
{
    "manifest_version": 2,
    "name": "TabLink",
    "version": "1",
    "description": "Open a every link in a new tab.",
    "author": "Thomas Carlsen",
    
    "browser_action": {
        "default_title": "Title preview value",
        "default_popup": "popup.html"
    },

    "content_scripts": [{
        "matches": ["<all_urls>"], 
        "js": ["contentScript.js"]
    }]
}

